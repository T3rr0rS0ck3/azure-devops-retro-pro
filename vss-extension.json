{
  "manifestVersion": 1,
  "id": "team-retro-pro",
  "version": "4.0.1",
  "name": "Team Retrospective Pro",
  "publisher": "T3rr0rS0ck3",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "RetroBoard in Azure DevOps im RetroTool-Stil: Sticky Notes, privater Bereich je Spalte, Export.",
  "categories": [
    "Azure Boards"
  ],
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "node_modules/azure-devops-extension-sdk",
      "addressable": true
    }
  ],
  "content": {
    "details": {
      "path": "README.html"
    }
  },
  "contributions": [
    {
      "id": "retro-pro-hub",
      "type": "ms.vss-web.hub",
      "targets": [
        "ms.vss-web.project-hub-group"
      ],
      "properties": {
        "name": "Retrospective",
        "uri": "dist/index.html"
      }
    }
  ]
}
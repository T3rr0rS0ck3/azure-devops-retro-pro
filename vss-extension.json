{
  "manifestVersion": 1,
  "id": "team-retro-pro",
  "version": "4.0.8",
  "name": "Team Retrospective Pro",
  "publisher": "T3rr0rS0ck3",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "RetroBoard in Azure DevOps im RetroTool-Stil: Sticky Notes, privater Bereich je Spalte, Export.",
  "categories": [
    "Azure Boards"
  ],
  "scopes": [
    "vso.extension",
    "vso.extension.data"
  ],
  "files": [
    {
      "path": "dist",
      "addressable": true
    }
  ],
  "content": {
    "details": {
      "path": "README.html"
    }
  },
  "contributions": [
    {
      "id": "retro-pro-hub",
      "type": "ms.vss-web.hub",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "Retrospective",
        "uri": "dist/index.html",
        "supportsScopes": [
          "project",
          "user"
        ]
      }
    }
  ]
}